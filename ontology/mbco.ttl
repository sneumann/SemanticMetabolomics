@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix mbco: <http://msbi.ipb-halle.de/rdf/ontology/mbco#> .

mbco:record a rdfs:Class ;
	rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
	rdfs:label "record" ;
	rdfs:comment "A class for massbank records" ;
	rdfs:subClassOf <http://semanticscience.org/resource/SIO_000088> .

mbco:mass_spectrometry_assay a rdfs:Class ;
	rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
	rdfs:label "mass_spectrometry_assay" ;
	rdfs:comment "The assay describing the mass spectrometry experiment that is described in the record" .

mbco:mass_spectrum a rdfs:Class ;
	rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
	rdfs:label "mass_spectrum" ;
	rdfs:comment "Information of the spectrum generated by an assay" .
	
mbco:peak a rdfs:Class ;
	rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
	rdfs:label "peak" ;
	rdfs:comment "A peak of the linked spectrum" ;
	rdfs:subClassOf <http://semanticscience.org/resource/SIO_011123> .
	
mbco:chemical_entity a rdfs:Class ;
	rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
	rdfs:label "chemical_entity" ;
	rdfs:comment "Contains meta-information about the chemical entity of the recorded spectrum" .
	
mbco:has_accession a rdf:Property ;
    rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "has_accession" ;
    rdfs:range xsd:string ;
	rdfs:comment "The accession number of the record in Massbank" ;
	rdfs:domain mbco:record .

mbco:hyperlink_record a rdf:Property ;
	rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
	rdfs:label "hyperlink_record" ;	
	rdfs:range mbco:string ;
	rdfs:domain mbco:record .
	
mbco:describes a rdf:Property ;
    rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "describes" ;
    rdfs:range mbco:mass_spectrometry_assay ;
	rdfs:comment "The assay described by the record" ;
	rdfs:domain mbco:record .

mbco:is_described_by a rdf:Property ;
    rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "is_described_by" ;
    rdfs:range mbco:record ;
	rdfs:comment "The record describing the record" ;
	rdfs:domain mbco:mass_spectrometry_assay .

mbco:ms_type a rdf:Property ;
    rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "ms_type" ;
    rdfs:range xsd:string ;
	rdfs:comment "The MS-Type with which the assay was generated" ;
	rdfs:domain mbco:mass_spectrometry_assay .
	
mbco:has_output a rdf:Property ;
    rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "has_output" ;
    rdfs:range mbco:mass_spectrum ;
	rdfs:comment "The spectrum generated by the linked assay" ;
	rdfs:domain mbco:mass_spectrometry_assay .

mbco:is_output_of a rdf:Property ;
    rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "is_output_of" ;
    rdfs:range mbco:mass_spectrometry_assay ;
	rdfs:comment "The assay generating the linked spectrum" ;
	rdfs:domain mbco:mass_spectrum .
	
mbco:identifies a rdf:Property ;
    rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "identifies" ;
    rdfs:range mbco:chemical_entity ;
	rdfs:comment "The chemical entity the was used to generate the spectrum" ;
	rdfs:domain mbco:mass_spectrum .
	
mbco:identified_by a rdf:Property ;
    rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "identified_by" ;
    rdfs:range mbco:mass_spectrum ;
	rdfs:comment "The spectrum which was made using the chemical entity" ;
	rdfs:domain mbco:chemical_entity .

mbco:name a rdf:Property ;
    rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "name" ;
    rdfs:range xsd:string ;
	rdfs:comment "The name of the chemical entity" ;
	rdfs:domain mbco:chemical_entity .
	
mbco:has_formula a rdf:Property ;
    rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "has_formula" ;
    rdfs:range xsd:string ;
	rdfs:comment "The formula of the chemical entity" ;
	rdfs:domain mbco:chemical_entity .
	
mbco:has_smiles a rdf:Property ;
    rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "has_smiles" ;
    rdfs:range xsd:string ;
	rdfs:comment "The smiles string describing the formula of the chemical entity" ;
	rdfs:domain mbco:chemical_entity .

mbco:chebi_link a rdf:Property ;
	rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
	rdfs:label "chebi_link" ;
    rdfs:range <http://bio2rdf.org/chebi_vocabulary:Resource> ;
	rdfs:comment "A link to the bio2rdf resource about the chemical entity" ;
	rdfs:domain mbco:chemical_entity .

mbco:instrument a rdf:Property ;
	rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "instrument" ;
    rdfs:range xsd:string ;
	rdfs:comment "The instrument with which the spectrum of the record was created" .	
	
mbco:has_constituent a rdf:Property ;
	rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
	rdfs:label "has_constituent" ;
	rdfs:range mbco:peak ;
	rdfs:domain mbco:mass_spectrum .

mbco:constituates a rdf:Property ;
	rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
	rdfs:label "constituates" ;
	rdfs:range mbco:mass_spectrum ;
	rdfs:domain mbco:peak .
	
mbco:encodes_mz a rdf:Property ;
	rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "encodes_mz" ;
    rdfs:range xsd:float ;
	rdfs:comment "The mass/charge ratio of a given peak stored as a float value" ;
	rdfs:domain mbco:peak .
	
mbco:has_intensity a rdf:Property ;
	rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "has_intensity" ;
    rdfs:range xsd:integer ;
	rdfs:comment "The intensity of a given peak stored as an integer value" ;
	rdfs:domain mbco:peak .
	
mbco:has_rel_intensity a rdf:Property ;
	rdfs:isDefinedBy <http://msbi.ipb-halle.de/rdf/ontology/mbco#> ;
    rdfs:label "has_rel_intensity" ;
    rdfs:range xsd:integer ;
	rdfs:comment "The relative intensity of a given peak, where the maximum intensity in a spectrum is defined as 999 while all others are put in relationship" ;
	rdfs:domain mbco:peak .